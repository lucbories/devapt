    "babel-preset-es2015": ">=6.6.0",
    "babel-preset-stage-0": ">=6.5.0",
    "babel-register": ">=6.14.0",
    "babelify": ">=7.2.0",
    "browserify": ">=13.1.1",
    "chai": ">=3.3.0",
    "chai-as-promised": ">=5.3.0",
    "chai-immutable": ">=1.3.0",
    "debounce": ">=1.0.0",
    "del": ">=2.2.0",
    "fs.extra": ">=1.3.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": ">=6.1.1",
    "gulp-cached": ">=1.1.0",
    "gulp-changed": ">=1.3.0",
    "gulp-concat": ">=2.6.0",
    "gulp-headerfooter": ">=1.0.3",
    "gulp-jsdoc3": ">=0.2.1",
    "gulp-livereload": ">=3.8.1",
    "gulp-load-plugins": ">=1.2.0",
    "gulp-remember": ">=0.3.0",
    "gulp-rename": ">=1.2.2",
    "gulp-showdown": ">=0.0.3",
    "gulp-sourcemaps": ">=1.6.0",
    "gulp-util": "^3.0.7",
    "ink-docstrap": ">=1.1.4",
    "mocha": ">=2.4.5",
    "requirejs": ">=2.1.20",
    "showdown": ">=1.4.2",
    "vinyl-buffer": ">=1.0.0",
