    "express-session": ">=1.8.2",
    "global": ">=4.3.1",
    "handlebars": ">=4.0.5",
    "hasher": ">=1.2.0",
    "helmet": ">=1.3.0",
    "html-to-vdom": ">=0.7.0",
    "immutable": ">=3.7.5",
    "jade": ">=1.11.0",
    "lodash": ">=4.16.4",
    "lowdb": ">=0.12.5",
    "moment": ">=2.13.0",
    "mustache": ">=2.2.1",
