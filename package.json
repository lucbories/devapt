    "passport-twitter": ">=1.0.2",
    "path-is-absolute": ">=1.0.0",
    "ramda": ">=0.21.0",
    "redux": ">=3.0.4",
    "redux-logger": ">=2.0.4",
    "redux-promise": ">=0.5.0",
    "redux-thunk": ">=1.0.0",
    "redux-undo": ">=0.5.0",
    "restify": ">=3.0.3",
    "semver": ">=5.3.0",
    "sequelize": ">=3.13.0",
    "simplebus": ">=0.0.4",
    "socket.io": ">=1.3.7",
    "typr": ">=0.2.1",
    "uuid": ">=3.0.0",
    "vdom-as-json": ">=1.0.9",
    "vdom-parser": ">=1.3.4",
    "vdom-virtualize": ">=2.0.0",
