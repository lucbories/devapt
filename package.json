{
  "name": "devapt",
  "version": "2.0.0",
  "description": "Developpers Application Toolkit - build application (backend, frontend, RESTfull, API, distributed nodes, reactive...) from configuration",
  "author": {
    "name": "Luc BORIES",
    "email": "luc.bories@gmail.com",
    "url": "https://github.com/lucbories"
  },
  "contributors": [],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucbories/devapt.git"
  },
  "bugs": {
    "url": "git+https://github.com/lucbories/devapt/issues"
  },
  "homepage": "https://github.com/lucbories/devapt#readme.md",
  "main": "dist/index.js",
  "engines": {
    "node": ">=4.4.4"
  },
  "directories": {
    "dist": "./dist"
  },
  "scripts": {
    "build": "gulp",
    "test": "mocha --recursive --reporter spec --compilers js:babel-register src/test",
    "test:common:base": "mocha --recursive --reporter spec --compilers js:babel-register src/test/unit/common/base",
    "test:common:cache": "mocha --recursive --reporter spec --compilers js:babel-register src/test/unit/common/cache",
    "test:common:datas": "mocha --recursive --reporter spec --compilers js:babel-register src/test/unit/common/datas",
    "test:common:rendering": "mocha --recursive --reporter spec --compilers js:babel-register src/test/unit/common/rendering",
    "test:common:topology": "mocha --recursive --reporter spec --compilers js:babel-register src/test/unit/common/topology",
    "test:common:utils": "mocha --recursive --reporter spec --compilers js:babel-register src/test/unit/common/utils",
    "test:empty": "mocha --reporter spec --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "test:unit:common": "npm run test:empty -- src/test/unit/common/base",
    "postversion": "unix2dos package.json",
    "commit": "unix2dos package.json && git status && pause && git commit -am ",
    "patch": "npm version patch && unix2dos package.json && git status && pause && git commit -am ",
    "minor": "npm version minor && unix2dos package.json && git status && pause && git commit -am ",
    "major": "npm version major && unix2dos package.json && git status && pause && git commit -am ",
    "push-dev": "git push devapt develop",
    "merge": "git checkout master && git merge develop && git push devapt master && git checkout develop",
    "release": "git push devapt develop && git checkout master && git merge develop && git push devapt master && git checkout develop"
  },
  "browser": [
    "sequelize",
    "restify"
  ],
  "keywords": [
    "server",
    "api",
    "rest",
    "devapt",
    "toolkit",
    "redux",
    "reactive",
    "declarative",
    "distributed",
    "responsive"
  ],
  "dependencies": {
    "acl": ">=0.4.9",
    "async": ">=1.0.0",
    "babel-core": ">=6.7.4",
    "baconjs": ">=0.7.84",
    "bunyan": ">=1.5.1",
    "cache-manager": ">=2.1.2",
    "chokidar": ">=1.4.3",
    "commander": ">=2.9.0",
    "compression": ">=1.6.1",
    "crossroads": ">=0.12.2",
    "epilogue": ">=0.5.2",
